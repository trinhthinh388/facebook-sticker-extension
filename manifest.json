{
  "name": "Facebook custom sticker",
  "version": "1.0",
  "description": "A sticker extension for Facebook",
  "manifest_version": 2,
  "content_scripts":[
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["jquery-3.5.1.min.js","content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
  }
}
